{
  "extends": ["airbnb", "prettier", "plugin:jest/recommended"],
  "plugins": ["jest", "react-hooks", "prettier"],
  "parser": "@babel/eslint-parser",
  "rules": {
    "prettier/prettier": ["error"],
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
