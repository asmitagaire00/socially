{
  "name": "socially",
  "version": "1.0.0",
  "description": "a simple social media application.",
  "main": "server.js",
  "scripts": {
    "server": "NODE_ENV=production node server.js",
    "server:watch": "NODE_ENV=development npx nodemon ./server.js",
    "server:test": "NODE_ENV=test npx nodemon ./server.js",
    "webpack-dev-server": "webpack serve --mode=development --progress",
    "webpack:watch": "npx webpack --watch --mode=development",
    "webpack:release": "npx webpack --mode=production",
    "build:production": "npm run css:build && npm run webpack:release",
    "css:build": "npx postcss ./src/public/css/main.css -o ./dist/main.css",
    "css:watch": "npx postcss --watch ./src/public/css/main.css -o ./dist/main.css",
    "test": "NODE_ENV=test jest -i --colors --verbose",
    "test:watch": "NODE_ENV=test jest -i --colors --verbose --watchAll",
    "cypress": "NODE_ENV=test cypress open",
    "cypress:development": "NODE_ENV=development cypress open --env fileConfig=development",
    "cypress:test": "NODE_ENV=test npx cypress run --env fileConfig=test",
    "cypress:production": "NODE_ENV=production npx cypress run --env fileConfig=production",
    "coverage": "jest -i --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "prepare": "husky install"
  },
  "author": "kishor",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.26.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "fs-extra": "^10.0.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.15",
    "nodemailer": "^6.6.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-form": "^8.3.7",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^3.4.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/eslint-parser": "^7.14.9",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "cypress": "^8.7.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.4.1",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
